Console.WriteLine("Введите натуральное двухзначное число");
string input = Console.ReadLine();
char first = input[0];
char second = input[1];
if (input.Length==2) {
if (first>second) 
{
    Console.WriteLine($"{first} больше чем {second}");
}
    else if (second>first){
    Console.WriteLine($"{second} больше чем {first}");
}
    else {
    Console.WriteLine("Числа одинаковые");
}
    Console.WriteLine();
}
else {
    Console.WriteLine($"Введите двухзначное число");
}
